{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended","plugin:jest/recommended"],
    "plugins": ["prettier", "jest"],

    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off"
    }
}
